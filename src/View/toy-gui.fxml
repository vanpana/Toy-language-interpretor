<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<HBox fx:id="mainwindow" fx:controller="View.GUIController" maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity" minWidth="-Infinity" prefHeight="608.0" prefWidth="1015.0"
      xmlns:fx="http://javafx.com/fxml/1">
    <ListView fx:id="statements" prefHeight="200.0" prefWidth="200.0">
        <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </HBox.margin>
    </ListView>
    <VBox HBox.hgrow="ALWAYS">
        <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0"/>
            </VBox.margin>
            <VBox prefHeight="193.0" prefWidth="172.0" HBox.hgrow="ALWAYS">
                <Label fx:id="noOfStatements" text="Label" VBox.vgrow="ALWAYS"/>
                <TableView fx:id="heaptable" prefHeight="176.0" prefWidth="227.0" VBox.vgrow="ALWAYS">
                    <columns>
                        <TableColumn fx:id="heaptable_address" prefWidth="121.92691040039062" text="Address"/>
                        <TableColumn fx:id="heaptable_value" prefWidth="110.07308959960938" text="Value"/>
                    </columns>
                    <VBox.margin>
                        <Insets/>
                    </VBox.margin>
                </TableView>
            </VBox>
            <ListView fx:id="outlist" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                <HBox.margin>
                    <Insets left="10.0" right="10.0" top="10.0"/>
                </HBox.margin>
            </ListView>
            <TableView fx:id="filetable" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                <columns>
                    <TableColumn prefWidth="69.17633056640625" text="ID"/>
                    <TableColumn prefWidth="190.8236083984375" text="Filename"/>
                </columns>
                <HBox.margin>
                    <Insets top="10.0"/>
                </HBox.margin>
            </TableView>
        </HBox>
        <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </VBox.margin>
            <ListView fx:id="prgstate_id" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                <HBox.margin>
                    <Insets right="10.0"/>
                </HBox.margin>
            </ListView>
            <TableView fx:id="symtable" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                <columns>
                    <TableColumn prefWidth="124.94186401367188" text="Name"/>
                    <TableColumn prefWidth="119.55810546875" text="Value"/>
                </columns>
                <HBox.margin>
                    <Insets left="10.0" right="10.0"/>
                </HBox.margin>
            </TableView>
            <ListView fx:id="exeStack" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                <HBox.margin>
                    <Insets left="10.0"/>
                </HBox.margin>
            </ListView>
        </HBox>
        <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
            <Button fx:id="onestep_button" alignment="CENTER" minWidth="-Infinity" mnemonicParsing="false"
                    text="One Step" HBox.hgrow="ALWAYS">
                <HBox.margin>
                    <Insets left="350.0" top="50.0"/>
                </HBox.margin>
            </Button>

            <Label fx:id="messages"></Label>
        </HBox>
    </VBox>
</HBox>
